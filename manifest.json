{
  "manifest_version": 3,
  "name": "Takea-Sponsor-Away",
  "version": "1.0.0",
  "description": "Automatically removes sponsored products on Takealot.com - Take a sponsor away!",
  "permissions": [],
  "host_permissions": [
    "*://*.takealot.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.takealot.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}

